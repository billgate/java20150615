<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

   <bean id="/board/list.do" 
         class="net.bitacademy.spring.controller.BoardListController"/>
   <bean id="/board/detail.do" 
         class="net.bitacademy.spring.controller.BoardDetailController"/>
   <bean id="/board/add.do" 
         class="net.bitacademy.spring.controller.BoardAddController"/>
   <bean id="/board/change.do" 
         class="net.bitacademy.spring.controller.BoardChangeController"/>
   <bean id="/board/remove.do" 
         class="net.bitacademy.spring.controller.BoardRemoveController"/>

</beans>

